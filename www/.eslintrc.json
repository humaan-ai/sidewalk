{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/prefer-stateless-function": "off",
        "react/prop-types": "off",
        "react/no-danger": "off"
    },
    "settings": {
        "import/core-modules": []
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "browser": true,
        "es6": true
    }
}
